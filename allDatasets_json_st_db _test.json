{
  "proto": {
    "id": "?s$anchor",
    "metadata": {
    	"property": "?p",
    	"value": "?o"    	    	 		
    }, 
    "spatial": {
    	"id":"$dct:spatial",
    	"geometryAsWKT":"$locn:geometry"
    },
    "dimensions": {
    	"id":"$qb:structure/qb:component/qb:dimension" ,
    	"label":"$rdfs:label"
    },
    "measures": {
    	"id":"$qb:structure/qb:component/qb:measure" ,
    	"label":"$rdfs:label" ,
    	"UoM":"$sdmx-attribute:unitMeasure"    	
    },
    "databases":{
    	"id":"$dcat:distribution/cybele:accessDatabase" ,
    	"label":"$rdfs:label" ,
    	"connectionString":"$cybele:connectionString" ,
    	"tables":{
    		"id":"$cybele:accessTable",
    		"label":"$cybele:tableName"
    	}
    }
  },
  "$from": "https://w3id.org/cybele/datasets/",
  "$where": [
    "?s a dcat:Dataset",
    "?s ?p ?o"    
  ],
  "$prefixes": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "dbo": "http://dbpedia.org/ontology/",
    "dcat": "http://www.w3.org/ns/dcat#",
    "dct": "http://purl.org/dc/terms/",
    "qb": "http://purl.org/linked-data/cube#",
		"sdmx-attribute": "http://purl.org/linked-data/sdmx/2009/attribute#",
		"cybele":	"http://w3id.org/cybele/model#",
		"locn":	"http://www.w3.org/ns/locn#" 
  },
  "$orderby": "?id",
  "$lang": "en",
  "grlc": {
    "summary": "Retrieve all information about CYBELE datasets, including only those with structure and database distribution",
    "endpoint": "https://www.foodie-cloud.org/sparql",
    "tags": [ "test" ],
    "method": "GET",
    "pagination": 100
  }
}
