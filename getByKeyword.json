{
  "proto": {
    "id": "?s",
    "metadata": {
    	"property": "?p",
    	"value": "?o"
    }
  },
  "$where": [
    "?s a dcat:Dataset",
    "?s ?p ?o",
    "?s ?p1 ?o1",
    "?o1 skos:prefLabel ?label1"
  ],
  "$filter": "bif:contains(?label1,  ?_keyword) || STRSTARTS (str(?s), str(cybeleBase:))",  
  "$prefixes": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "dct": "http://purl.org/dc/terms/",
    "dcat": "http://www.w3.org/ns/dcat#",
    "skos": "http://www.w3.org/2004/02/skos/core#",
    "cybeleBase": "https://w3id.org/cybele/",
    "bif": "bif:"
  },
  "$orderby": "?id",
  "$lang": "en",
  "$limit": 50,
  "grlc": {
    "summary": "Retrieve all information about CYBELE datasets including the provided keyword, e.g., greece, in the dataset title or connected resources' labels",
    "endpoint": "https://www.foodie-cloud.org/sparql",
    "tags": [ "json" ],
    "method": "GET",
    "pagination": 50
  }
}