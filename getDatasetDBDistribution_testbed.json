{
  "proto": {
    "id": "?_resource_iri",
    "distribution":{
    	"id":"?distribution",
    	"databases":{
    		"id":"?database" ,
    		"label":"?databaseLabel",
    		"tables":{
    			"id":"?databaseTable",
    			"label":"?tableName"
    		}
    	}
    }
  },
  "$where": [    
    "GRAPH ?g { ?_resource_iri dcat:distribution ?distribution . ?distribution cybele:accessDatabase|cybele_sec:accessDatabase ?database . ?database rdfs:label ?databaseLabel . ?database cybele:connectionString|cybele_sec:connectionString ?connectionString . ?database cybele:accessTable|cybele_sec:accessTable ?databaseTable . ?databaseTable cybele:tableName|cybele_sec:tableName ?tableName }"
  ],
  "$filter": "STRSTARTS(STR(?g),str(?_testbed))",
  "$prefixes": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "dct": "http://purl.org/dc/terms/",
    "dcat": "http://www.w3.org/ns/dcat#",
    "foaf": "http://xmlns.com/foaf/0.1/",
    "cybele":	"http://w3id.org/cybele/model#",
    "cybele_sec":	"https://w3id.org/cybele/model#"
  },
  "$lang": "en",
  "$limit": 100,
  "grlc": {
    "summary": "Retrieves the database distribution information about a particular dataset (e.g., https://w3id.org/cybele/dataset/id/dataset_2)",
    "endpoint": "https://www.foodie-cloud.org/sparql",
    "tags": [ "testbed" ],
    "method": "GET",
    "pagination": 100
    }
}