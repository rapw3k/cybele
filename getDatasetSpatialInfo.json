{
  "proto": {
    "id": "?_resource_iri",    
    "spatial": {
    	"id":"?spatial$anchor",
    	"geometryAsWKT":"$locn:geometry"
    }
  },
  "$where": [    
    "?_resource_iri dct:spatial ?spatial"    
  ],
  "$prefixes": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "dct": "http://purl.org/dc/terms/",
    "dcat": "http://www.w3.org/ns/dcat#",
    "foaf": "http://xmlns.com/foaf/0.1/",
    "locn":	"http://www.w3.org/ns/locn#" 
  },
  "$lang": "en",
  "$limit": 100,
  "grlc": {
    "summary": "Retrieves the spatial information about a particular dataset (e.g., https://w3id.org/cybele/dataset/id/dataset_2)",
    "endpoint": "https://www.foodie-cloud.org/sparql?default-graph-uri=https://w3id.org/cybele/datasets/",
    "tags": [ "json" ],
    "method": "GET",
    "pagination": 100
    }
}