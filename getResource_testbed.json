{
  "proto": {
    "id": "?_resource_iri",
    "metadata": {
    	"property": "?p",
    	"value": "?o"
    }
  },
  "$where": [
    "?_resource_iri ?p ?o",
    "GRAPH ?g { ?_resource_iri a ?c }"
  ],
  "$filter": "STRSTARTS(STR(?g),str(?_testbed))",
  "$prefixes": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "dct": "http://purl.org/dc/terms/",
    "dcat": "http://www.w3.org/ns/dcat#",
    "foaf": "http://xmlns.com/foaf/0.1/"
  },
  "$lang": "en",
  "$limit": 50,
  "grlc": {
    "summary": "Retrieves all the information about a particular resource (e.g., dataset (https://w3id.org/cybele/dataset/id/bd2c8216-fd62-4740-92b4-39511af4336c), publisher (https://w3id.org/cybele/agent/id/8f6332ab-bb45-4199-a339-3f2710d4070d), etc.)",
    "endpoint": "https://www.foodie-cloud.org/sparql",
    "tags": [ "testbed" ],
    "method": "GET",
    "pagination": 50
    }
}